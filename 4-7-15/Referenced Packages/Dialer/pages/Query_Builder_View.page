                                        var limitCallsToLabel = document.getElementById("limit_calls_to_label");
                                        if(updatedLabel == 'all') {
                                            updatedText = 'No Limit';
                                        }
                                        else if(updatedLabel == 'owner') {
                                            updatedText = 'Record Owner';
                                        }
                                        else if(updatedLabel == 'account_teams') {
                                            updatedText = 'Account Team';
                                        }
                                        else if(!updatedLabel) {
