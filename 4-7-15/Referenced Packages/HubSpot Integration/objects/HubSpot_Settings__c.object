        <deprecated>false</deprecated>
        <description>Determine what activities to store.</description>
        <externalId>false</externalId>
        <inlineHelpText>Please see the activity settings in HubSpot under Contacts -&gt; Settings -&gt; Salesforce Connector -&gt; Other Settings.</inlineHelpText>
        <label>Deprecated - Store HubSpot Activities</label>
        <picklist>
            <picklistValues>
                <fullName>Store both Form Submissions and Emails</fullName>
                <default>true</default>
            </picklistValues>
            <picklistValues>
                <fullName>Store only Form Submissions</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Store only Emails</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Do not store any HubSpot Activities</fullName>
                <default>false</default>
            </picklistValues>
            <sorted>false</sorted>
        </picklist>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Picklist</type>
