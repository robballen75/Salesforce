    <fields>
        <fullName>De_duplication_Rule__c</fullName>
        <deprecated>false</deprecated>
        <description>Determine whether HubSpot should de-duplicate incoming new HubSpot Leads</description>
        <externalId>false</externalId>
        <label>De-duplication Rule</label>
        <picklist>
            <picklistValues>
                <fullName>De-duplicate against existing Leads and Contacts</fullName>
                <default>true</default>
            </picklistValues>
            <picklistValues>
                <fullName>De-duplicate against existing Leads only</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Do not de-duplicate</fullName>
                <default>false</default>
            </picklistValues>
            <sorted>false</sorted>
        </picklist>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Picklist</type>
    </fields>
    <fields>
        <fullName>HAPI_Domain__c</fullName>
        <defaultValue>&quot;hubapi.com&quot;</defaultValue>
        <deprecated>false</deprecated>
        <description>The domain to use for calls to the HubSpot API.  Defaults to hubapi.com and should not be changed.</description>
        <externalId>true</externalId>
        <label>HAPI Domain</label>
        <length>50</length>
        <required>true</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
