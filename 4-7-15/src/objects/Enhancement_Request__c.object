    <validationRules>
        <fullName>ERVerifyFixedInBuild</fullName>
        <active>true</active>
        <description>Verify that a value is selected for the Fixed In Build field if the resolution is &quot;fixed&quot;</description>
        <errorConditionFormula>AND ( 
ISPICKVAL( ER_Resolution__c  ,&quot;Fixed&quot;), 
TEXT(  ER_Fixed_In_Version__c  )=&quot;&quot; 
)</errorConditionFormula>
        <errorMessage>Please specify the Fixed In Version.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ERVerifyResolutionOnClose</fullName>
        <active>true</active>
        <description>Ensure that there is a resolution set if status is set to Closed</description>
        <errorConditionFormula>AND ( 
ISPICKVAL(ER_Status__c ,&quot;Closed&quot;), 
TEXT( ER_Resolution__c )=&quot;&quot; 
)</errorConditionFormula>
        <errorDisplayField>ER_Resolution__c</errorDisplayField>
        <errorMessage>Please select the appropriate ER Resolution.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SetAssignedToRelease</fullName>
        <active>true</active>
        <errorConditionFormula>AND ( 
ISPICKVAL( ER_Status__c ,&quot;Assigned&quot;), 
TEXT( ER_Assigned_to_release__c )=&quot;&quot; 
)</errorConditionFormula>
        <errorDisplayField>ER_Assigned_to_release__c</errorDisplayField>
        <errorMessage>Please select the release to which the ER has been assigned.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Effort_Estimate</fullName>
        <active>true</active>
        <description>Ensure Effort Estimate is a valid value less than 5.</description>
        <errorConditionFormula>Effort_Estimate__c  &gt; 5</errorConditionFormula>
        <errorMessage>Effort Estimate must be a value between 1 and 5.
1:  Less than a day
2:  Less than a week
3:  Less than a month
4:  Less than 3 months
5:  More than 3 months</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Penalty</fullName>
        <active>true</active>
        <description>Ensure Penalty is a valid value less than 5.</description>
        <errorConditionFormula>Penalty__c &gt; 5</errorConditionFormula>
        <errorMessage>Penalty must be a value between 1 and 5.
1: Missed competitive opp.
2: Annoying to Admin 
3: Annoying to end user 
4: Returns misleading results 
5: Corrupts data</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Pervasiveness</fullName>
