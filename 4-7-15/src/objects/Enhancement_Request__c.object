    </validationRules>
    <validationRules>
        <fullName>Validate_Process_Impact</fullName>
        <active>true</active>
        <description>Ensure Effort Estimate is a valid value less than 5.</description>
        <errorConditionFormula>Process_Impact__c   &gt; 5</errorConditionFormula>
